generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Airplane {
  id        Int      @id @default(autoincrement())
  modelName String   @db.VarChar(200)
  capacity  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([modelName])
}

model City {
  id        Int      @id @default(autoincrement())
  name      String   @unique @db.VarChar(200)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([name])
}
